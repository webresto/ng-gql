{
  "name": "@webresto/ng-gql",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build --watch",
    "build-update-deps-script": "tsc updatePackageVersion.ts --skipLibCheck",
    "updatePackageVersion": "node updatePackageVersion.js ./projects/webresto/ng-gql/package.json",
    "build": "ng build",
    "upd": "npm update --save --force --legacy-peer-deps",
    "upd-up": "npm run upd && npm run updatePackageVersion && npm run build:local",
    "build:local": "ng build && npm run gen-doc",
    "watch:axrl": "ng build --watch --configuration axrl",
    "watch:axrl:nadym": "ng build --watch --configuration axrl-nadym",
    "gen-doc": "typedoc --tsconfig projects/webresto/ng-gql/tsconfig.lib.prod.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@apollo/client": "^3.7.17",
    "@axrl/common": "^1.0.31",
    "@axrl/ngx-extended-form-builder": "^1.2.10",
    "apollo-angular": "^5.0.0",
    "apollo3-cache-persist": "^0.14.1",
    "crypto-puzzle": "^4.0.2",
    "graphql": "^16.7.1",
    "rxjs": "^7.8.1",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.6.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "^16.1.5",
    "@angular/compiler-cli": "^16.1.6",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "ng-packagr": "^16.1.0",
    "prettier": "^3.0.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.9.5"
  }
}
