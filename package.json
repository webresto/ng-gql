{
  "name": "@webresto/ng-gql",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build --watch",
    "updatePackageVersion": "node updatePackageVersion.js",
    "build": "ng build",
    "build:local": "npm run build && npm run gen-doc",
    "watch:axrl": "ng build --watch --configuration axrl",
    "watch:axrl:nadym": "ng build --watch --configuration axrl-nadym",
    "gen-doc": "typedoc --tsconfig projects/webresto/ng-gql/tsconfig.lib.prod.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^13.3.3",
    "@angular/compiler": "^13.3.3",
    "@angular/core": "^13.3.3",
    "@apollo/client": "^3.5.10",
    "@axrl/ngx-extended-form-builder": "^1.0.9",
    "apollo-angular": "^3.0.1",
    "graphql": "^16.3.0",
    "rxjs": "^7.5.5",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.3",
    "@angular/cli": "^13.3.3",
    "@angular/compiler-cli": "^13.3.3",
    "@types/node": "^17.0.25",
    "ng-packagr": "^13.3.1",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.0",
    "typescript": "^4.6.3"
  }
}
