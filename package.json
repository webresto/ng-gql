{
  "name": "@webresto/ng-gql",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build --watch",
    "build-update-deps-script":"tsc updatePackageVersion.ts --skipLibCheck",
    "updatePackageVersion": "node updatePackageVersion.js",
    "build": "ng build",
    "upd": "npm update --save --force",
    "build:local": "ng build && npm run gen-doc",
    "watch:axrl": "ng build --watch --configuration axrl",
    "watch:axrl:nadym": "ng build --watch --configuration axrl-nadym",
    "gen-doc": "typedoc --tsconfig projects/webresto/ng-gql/tsconfig.lib.prod.json"
  },
  "private": true,
  "peerDependencies": {
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/forms": "^15.0.1",
    "@apollo/client": "^3.7.1",
    "@axrl/ngx-extended-form-builder": "^1.2.6",
    "apollo-angular": "^4.1.1",
    "apollo3-cache-persist": "^0.14.1",
    "graphql": "^16.6.0",
    "rxjs": "^7.5.7",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.1",
    "@types/node": "^18.11.9",
    "ng-packagr": "^15.0.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  }
}
