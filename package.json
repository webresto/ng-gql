{
  "name": "@webresto/ng-gql",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build --watch",
    "build-update-deps-script": "tsc updatePackageVersion.ts --skipLibCheck",
    "updatePackageVersion": "node updatePackageVersion.js ./projects/webresto/ng-gql/package.json",
    "build": "ng build",
    "upd": "npm update --save --force --legacy-peer-deps",
    "upd-up": "npm run upd && npm run updatePackageVersion && npm run build:local",
    "build:local": "ng build && npm run gen-doc",
    "watch:axrl": "ng build --watch --configuration axrl",
    "watch:axrl:nadym": "ng build --watch --configuration axrl-nadym",
    "gen-doc": "typedoc --tsconfig projects/webresto/ng-gql/tsconfig.lib.prod.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@apollo/client": "^3.7.17",
    "@axrl/common": "^1.0.32",
    "@axrl/ngx-extended-form-builder": "^1.2.10",
    "apollo-angular": "^4.2.1",
    "apollo3-cache-persist": "^0.14.1",
    "crypto-puzzle": "^4.0.2",
    "graphql": "^16.7.1",
    "rxjs": "^7.8.1",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.6.1",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@types/node": "^18.17.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "ng-packagr": "^15.2.2",
    "prettier": "^3.0.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^5.1.6"
  }
}
