{
  "name": "@webresto/ng-gql",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build --watch",
    "updatePackageVersion": "node updatePackageVersion.js",
    "build": "ng build",
    "upd": "npm update --save --force --legacy-peer-deps",
    "build:local": "ng build && npm run gen-doc",
    "watch:axrl": "ng build --watch --configuration axrl",
    "watch:axrl:nadym": "ng build --watch --configuration axrl-nadym",
    "gen-doc": "typedoc --tsconfig projects/webresto/ng-gql/tsconfig.lib.prod.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^14.0.6",
    "@angular/compiler": "^14.0.6",
    "@angular/core": "^14.0.6",
    "@angular/forms": "^14.0.6",
    "@apollo/client": "^3.6.9",
    "@axrl/ngx-extended-form-builder": "^1.1.9",
    "apollo-angular": "^3.0.1",
    "apollo3-cache-persist": "^0.14.1",
    "graphql": "^16.5.0",
    "rxjs": "^7.5.6",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.6",
    "@angular/cli": "^14.0.6",
    "@angular/compiler-cli": "^14.0.6",
    "@types/node": "^17.0.45",
    "ng-packagr": "^14.0.3",
    "typedoc": "^0.22.18",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "^4.7.4"
  }
}
