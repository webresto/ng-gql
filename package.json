    "@axrl/ngx-extended-form-builder": "^1.0.9",
    "apollo-angular": "^3.0.0",
    "graphql": "^16.3.0",
    "rxjs": "^7.5.4",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.4",
    "@angular/cli": "^13.2.4",
    "@angular/compiler-cli": "^13.2.3",
    "@types/node": "^17.0.20",
    "ng-packagr": "^13.2.1",
    "typedoc": "^0.22.12",
