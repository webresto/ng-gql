{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "cache": {
      "enabled": false
    },
    "analytics": false
  },
  "projects": {
    "@webresto/ng-gql": {
      "projectType": "library",
      "root": "projects/webresto/ng-gql",
      "sourceRoot": "projects/webresto/ng-gql/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/webresto/ng-gql/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/webresto/ng-gql/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/webresto/ng-gql/tsconfig.lib.json"
            },
            "axrl": {
              "project": "projects/webresto/ng-gql/ng-package-axrl.json",
              "tsConfig": "projects/webresto/ng-gql/tsconfig.lib.json"
            },
            "axrl-nadym": {
              "project": "projects/webresto/ng-gql/ng-package-axrl-nadym.json",
              "tsConfig": "projects/webresto/ng-gql/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/webresto/ng-gql/src/test.ts",
            "tsConfig": "projects/webresto/ng-gql/tsconfig.spec.json",
            "karmaConfig": "projects/webresto/ng-gql/karma.conf.js"
          }
        }
      }
    }
  }
}